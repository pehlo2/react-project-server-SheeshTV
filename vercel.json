{
    "version": 2,
    "builds": [
      { "src": "./socket.js", "use": "@vercel/node" },
      { "src": "./index.js", "use": "@vercel/node" }
    ],
    "rewrites": [
      { "source": "/socket/(.*)", "destination": "/socket.js" },
      { "source": "/api/(.*)", "destination": "/index.js" }
    ]
    
  }